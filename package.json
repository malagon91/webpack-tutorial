{
  "name": "webpack-curse",
  "version": "1.0.0",
  "description": "curso ce webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack script.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:styles": "webpack --config ./css-styles-loader/webpack.config.js",
    "build:extract": "webpack --config ./extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiple_entry/webpack.config.js",
    "serve": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:loader": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Miguel Malagon",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "url-loader": "^1.1.0",
    "webpack": "^3.8.1",
